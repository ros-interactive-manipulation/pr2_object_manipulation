<robot name="cup_model">
  <link name="cup">
    <inertial>
      <mass value="0.2" />
      <!-- center of mass (com) is defined w.r.t. link local coordinate system -->
      <origin xyz="0 0 0.06" /> 
      <inertia  ixx="0.1" ixy="0.0"  ixz="0.0"  iyy="0.1"  iyz="0.0"  izz="0.1" />
    </inertial>
    <visual>
      <!-- visual origin is defined w.r.t. link local coordinate system -->
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://manipulation_worlds/meshes/cup.dae" scale=".1 .1 .1" />
      </geometry>
    </visual>
    <collision>
      <!-- collision origin is defined w.r.t. link local coordinate system -->
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://manipulation_worlds/meshes/cup.dae" scale=".1 .1 .1" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="cup">
    <turnGravityOff>false</turnGravityOff>
    <genTexCoord>true</genTexCoord>
    <kp>100000000.0</kp>
    <kd>1000000.0</kd>
    <controller:gazebo_ros_grasp_hack name="cup_grasp_hack_controller" plugin="libgazebo_ros_grasp_hack.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>1000</updateRate>
    <controller:gazebo_ros_grasp_hack/>
  </gazebo>
</robot>
